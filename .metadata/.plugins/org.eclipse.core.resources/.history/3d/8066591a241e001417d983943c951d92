	public static void displayMatrix(Matrix m){
		for (int i = 0; i < m.getRowDimension(); i++) {
			for (int j = 0; j < m.getColumnDimension(); j++) {
				System.out.print(m.get(i, j)+" ");
			}
			System.out.println();
		}
	}
}
